@startuml
title Diagrama de Estructura Compuesta - Carrito de Compras

class Carrito {
  -id: int
  -fecha: Date
  +calcularTotal()
}

class LineaCarrito {
  -cantidad: int
  +subtotal()
}

class Producto {
  -id: int
  -nombre: String
  -precio: double
}

Carrito *-- LineaCarrito
LineaCarrito *-- Producto

@enduml
